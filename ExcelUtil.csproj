    <Version>2.0.3</Version>
    <Authors>Somogyi Krisztian</Authors>
    <Description>A utility lib, to read checkbox control and radio button control status from *.xlsx</Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <RepositoryUrl></RepositoryUrl>
    <Company>somogyi.xyz</Company>
